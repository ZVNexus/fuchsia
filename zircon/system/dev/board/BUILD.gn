group("board") {
  testonly = true
  deps = [
    "integration-test",
    "machina",
    "qemu",
    "test",
  ]
  if (current_cpu == "arm64") {
    deps += [
      "as370",
      "astro",
      "sargo",
      "gauss",
      "hikey960",
      "msm8x53-som",
      "mt8167s_ref",
      "sherlock",
      "vim2",
    ]
  } else if (current_cpu == "x64") {
    deps += [ "x86" ]
  }
}
